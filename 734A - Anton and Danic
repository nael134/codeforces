#include<iostream>
using namespace std;
int main(){
    int n,count=0,mount=0;
    string s;
    cin>>n>>s;
    for(int i=0; i<s.length(); i++)
    {
        if(s[i] == 'A')
        {
            count++;
        }
        else
            mount++;
    }
    if(mount>count)
        cout<<"Danik";
    else if(count>mount)
        cout<<"Anton";
    else
        cout<<"Friendship";
 
}
