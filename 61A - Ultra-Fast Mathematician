#include<iostream>
using namespace std;
int main(){
    string a,b;
    cin>>a>>b;
    string count="";
    for(int i=0; i<a.length(); i++)
    {
        if(a[i] == '0' && b[i] == '1' || a[i] == '1' && b[i] == '0' )
        {
            count += '1';
        }
        else    
            count += '0';
    }
    cout<<count;
}
