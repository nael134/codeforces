#include<iostream>
using namespace std;
int main(){
    int a;
    cin>>a;
    int count=0;
    if(a%2==0){
        cout<<a/2<<"\n";
        for(int i=0; i<a/2; i++){
            cout<<2<<" ";
        }
    }
    else{
        cout<<(a/2)<<"\n";
        a-=3;
        for(int i=0; i<a/2; i++){
            cout<<2<<" ";
        }
        cout<<3;
    }
}
