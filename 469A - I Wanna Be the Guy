#include<iostream>
#include<set>
using namespace std;
int main(){
    int n;
    cin>>n;
    set<int> l;
    int a,b;
    cin>>a;
    for(int i=0; i<a; i++){
        int x;
        cin>>x;
        l.insert(x);
    }
    cin>>b;
    for(int i=0; i<b; i++){
        int x; 
        cin>>x;
        l.insert(x);
    }
    if(l.size() == n){
        cout<<"I become the guy.";
    }
    else
        cout<<"Oh, my keyboard!";
}
