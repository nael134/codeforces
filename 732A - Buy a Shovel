#include<iostream>
using namespace std;
 
int main() {
    int k, r;
    cin >> k >> r;
 
    if (k % 10 == 0) {
        cout << 1;
        return 0;
    }
 
    
	    for (int i = 1; i < 1000; i++) {
	       if (((i * k) - r) % 10 == 0 || (i * k) % 10 == 0) {
	           cout << i;
	           return 0;
	       }
	    }
}
