#include<iostream>
#include<string>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        string s;
        cin>>s;
 
        string d = s.substr(0,s.length()/2);
        string b = s.substr(s.length()/2);
 
        if(d==b){cout<<"YES\n";}
        else cout<<"NO\n";
 
    }
 
}
