#include<iostream>
#include<algorithm>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--)
    {
        string s;
        cin>>s;
        int n=0,k=0;
        
        for(int i=0; i<3; i++){
            n += s[i] - '0';
        }
        for(int i=3; i<6; i++){
            k += s[i] - '0';
        }
        if(n==k)cout<<"YES\n";
        else cout<<"NO\n";
    }
}
