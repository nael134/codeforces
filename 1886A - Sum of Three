#include<iostream>
using namespace std;
int main(){
    int t,n;
    cin>>t;
    while(t--)
    {
        cin>>n;
        if(n==9 || n<7)
        {
            cout<<"NO\n";
        } else{
            if(n%3 == 0)
            {
                cout<<"YES\n"<<1<<" "<<4<<" "<<n-5<<"\n";
            } else
                cout<<"YES\n"<<1<<" "<<2<<" "<<n-3<<"\n";
        }
    }
}
